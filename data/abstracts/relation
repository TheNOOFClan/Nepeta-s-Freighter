[ABSTRACT:RELATION]
  [REQ:VALUE:NAME]
  [REQ:VALUE:SYMBOL]
  
  [REQ:VALUE:LEFT:TYPE]
  [REQ:VALUE:RIGHT:TYPE]
  
  [OPT:EXTANT:SHIPPABLE]
  [OPT:EXTANT:CRUSHABLE]
  [OPT:EXTANT:UTILITY]
  
  [OPT:FUNC:CREATE:SHIP]
  [OPT:FUNC:CRUSH:SHIP]
  
  [OPT:LIST:INCOMPATIBLE:NONE:RELATION|{RELATION:SYMBOL}|{RELATION:NAME}]
